{
    "manifest_version": 2,
    "name": "Stephan Griesel Launcher",
    "description": "Quick Launch to my basic CSS Grid website & resources",
    "version": "1.0.0",
    "icon": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}